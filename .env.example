# PageClassifier Configuration
MIN_FINANCIAL_TERMS=7
ZSCORE_THRESHOLD=1.0
GAP_PERCENTAGE_THRESHOLD=30.0

=
# AI Service Configuration
DEFAULT_AI_SERVICE=gemini  # Options: openai, gemini, vertexai
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENROUTER_API_KEY=

=
# AI Model Configuration
OpenAI_AI_MODEL=xxx
Gemini_AI_MODEL=xxx
OpenRouter_AI_MODEL=xxx
VertexAI_AI_MODEL=xxx


# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id
GOOGLE_CLOUD_VERTEX_LOCATION=us-west4

=
# Azure Document Intelligence OCR Configuration
AZURE_DOCUMENT_INTELLIGENCE_KEY=your_azure_document_intelligence_key_here
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=your_azure_document_intelligence_endpoint_here

=

=
# AWS Configuration
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_BUCKET_NAME = "results-sharing"  # Your bucket name
AWS_FOLDER_PREFIX = "results"  # Folder within the bucket

=
# OCR Service Configuration
DEFAULT_OCR_SERVICE=azure_document_intelligence  # Options: mistral, azure_document_intelligence

=
# File Processing Configuration
TEMP_DIR=temp
FILE_CLEANUP_AGE=3600

=
# Telegram Configuration
TELEGRAM_ENABLED=true
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

=
# Set to true to send S3 URLs to Telegram, false to send files directly
TELEGRAM_USE_S3_URL=true

=
# Testing Mode Configuration
# Set to true to bypass all Telegram notifications during testing
TESTING_MODE=true

=
# Result Type Configuration
RESULT_TYPE=multi_page  # Options: single_page, multi_page
MULTI_PAGE_RESULT_TYPE=consolidated  # Used when RESULT_TYPE=multi_page , options: consolidated, standalone

